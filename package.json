{
  "name": "phylotree",
  "version": "0.2.0-alpha.1",
  "main": "phylotree.js",
  "keywords": [
    "phylogenetics",
    "phylogeny",
    "d3",
    "branching",
    "tree"
  ],
  "ignore": [
    "index.html"
  ],
  "dependencies": {
    "bootstrap": "4",
    "d3": "5",
    "font-awesome": "4.x",
    "jquery": "3",
    "lodash": "^4.17.11",
    "underscore": "1.x",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "eslint": "^5.3.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "jsdom": "9.12.x",
    "mocha": "^5.2.0",
    "prettier": "^1.14.2",
    "rollup": "^0.65.2",
    "rollup-plugin-terser": "^2.0.2",
    "rollup-watch": "^4.3.1",
    "should": "^11.2.1",
    "tape": "4",
    "uglify-js": "2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "yarn": "^1.9.4"
  },
  "scripts": {
    "docs": "sphinx-build -b html docs documentation",
    "lint": "eslint --ext .js --ignore-pattern data,docs,images,node_modules ./",
    "dev": "rm -rf build && mkdir build && rollup -m -c -w -f umd -o build/phylotree.js -- index.js",
    "pretest": "rm -rf build && mkdir build && rollup -c -f umd -o build/phylotree.js -- index.js",
    "test": "tape 'test/*-test.js'",
    "prepublish": "npm run test && uglifyjs build/phylotree.js -c -m -o build/phylotree.min.js",
    "postpublish": "zip -j build/phylotree.js.zip -- LICENSE README.md build/phylotree.js build/phylotree.min.js"
  },
  "files": [
    "phylotree.css",
    "phylotree.js"
  ]
}
